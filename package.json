{
  "name": "typometer",
  "main": "typometer.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run build:js",
    "build:js": "jspm build typometer.ts dist/assets/javascript/index.js",
    "env": "env",
    "prebuild": "npm run clean -s && mkdir -p dist/assets/javascript/",
    "postbuild": "cp -r assets/ dist/ && cp index.bundle.html dist/index.html",
    "serve": "http-server dist/ -c-1 -S -C cert.pem -K key.pem -o",
    "serve:dev": "http-server -c-1 -S -C cert.pem -K key.pem -o",
    "start": "npm run build && npm run serve",
    "start:dev": "npm run serve:dev",
    "test": "echo 'Bad scoundrel meself :('"
  },
  "dependencies": {},
  "devDependencies": {
    "@cycle/dom": "npm:@cycle/dom@^22.0.0",
    "@cycle/http": "npm:@cycle/http@^15.0.0",
    "@cycle/isolate": "npm:@cycle/isolate@^4.1.0",
    "@cycle/run": "npm:@cycle/run@^5.1.0",
    "browserify": "^16.2.3",
    "classnames": "npm:classnames@^2.2.6",
    "cycle-onionify": "npm:cycle-onionify@^6.1.0",
    "most": "npm:most@^1.0.1",
    "tsify": "^4.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^2.6.2",
    "vinyl-source-stream": "^2.0.0",
    "xstream": "npm:xstream@^11.7.0"
  }
}
